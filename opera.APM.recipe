<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
	<dict>
		<key>Description</key>
		<string>Move opera package in JAMF PST</string>
		<key>Identifier</key>
		<string>com.github.jherrinjr.APM.opera</string>
		<key>Input</key>
		<dict></dict>
		<key>MinimumVersion</key>
		<string>1.0.0</string>
