		<array>
			<dict>
				<key>Arguments</key>
				<dict>
					<key>applicationTitle</key>
					<string>Opera</string>
					<key>generalPolicyName</key>
					<string>Install Latest Opera</string>
					<key>patchSoftwareTitle</key>
					<string>Opera</string>
					<key>gammaDistributionMethod</key>
					<string>self service</string>
					<key>productionDelay</key>
					<integer>7</integer>
					<key>prodDistributionMethod</key>
					<string>prompt</string>
				</dict>
				<key>Processor</key>
				<string>com.github.thejacobsales.APM</string>
			</dict>
		</array>
	</dict>
